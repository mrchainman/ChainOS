{
"languageserver": {
  "dockerfile": {
    "command": "docker-langserver",
    "filetypes": ["dockerfile"],
    "args": ["--stdio"]
  },
  "digestif": {
    "command": "digestif",
    "filetypes": ["tex", "plaintex", "context"]
  },
  "sql": {
    "command": "sql-language-server",
    "args" : ["up", "--method", "stdio"],
    "filetypes": ["sql", "mysql"]
    }
}
}
